{
    "name": "combizera/wordpress-to-laravel-migrator",
    "description": "Package for migrating WordPress posts to laravel",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Combizera\\WpMigration\\": "src/"
        }
    },
    "require": {
        "php": ">= 8.2"
    },
    "require-dev": {
        "laravel/pint": "^1.22"
    },
    "authors": [
        {
            "name": "Combizera",
            "email": "ycombi349@gmail.com"
        }
    ],
    "scripts": {
        "format": "pint"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Combizera\\WpMigration\\WpMigrationServiceProvider"
            ]
        }
    }
}
